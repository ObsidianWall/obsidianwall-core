


{
  "problemMatcher": [
    {
      "owner": "tflint",
      "pattern": [
        {
          "regexp": "^(.*?):(\\d+):(\\d+):\\s+\\[(ERROR|WARNING)\\]\\s+(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "severity": 4,
          "message": 5
        }
      ]
    }
  ]
}

